shift_str = input()
shift = int(shift_str.strip())
phrase = input()

START_CODE = 0x1F600
END_CODE = 0x1F64F
ALPHABET_SIZE = END_CODE - START_CODE + 1

normalized_shift = shift % ALPHABET_SIZE

encrypted_result = []

for char in phrase:
    code_point = ord(char)

    if START_CODE <= code_point <= END_CODE:
        index = code_point - START_CODE
        new_index = (index + normalized_shift) % ALPHABET_SIZE
        new_code_point = new_index + START_CODE
        
        encrypted_result.append(chr(new_code_point))
    else:
        encrypted_result.append(char)

print("".join(encrypted_result))
