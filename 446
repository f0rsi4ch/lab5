try:
    user_input = input("Введіть цілі числа через пробіл: ")
    
    numbers = [int(num) for num in user_input.split() if num]
    
    counts = {} 
    
    for num in numbers:
        if num in counts:
            counts[num] += 1
        else:
            counts[num] = 1
    
    total_pairs = 0
    
    for count in counts.values():
        if count >= 2:
            pairs_for_this_number = count * (count - 1) // 2
            total_pairs += pairs_for_this_number
    
    print(total_pairs)

except ValueError:
    print("Помилка: Необхідно ввести лише цілі числа, розділені пробілами.")
